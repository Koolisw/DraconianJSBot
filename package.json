{
  "private": true,
  "name": "draconian-bot",
  "version": "2.0.0",
  "description": "Multi-Purpose & Productivity concentrated Discord BOT.",
  "license": "MIT",
  "author": "cktsun1031",
  "homepage": "https://github.com/cktsun1031/DraconianBot#readme",
  "repository": {
    "url": "git+https://github.com/cktsun1031/DraconianBot.git"
  },
  "bugs": {
    "url": "https://github.com/cktsun1031/DraconianBot/issues"
  },
  "main": "src/index.ts",
  "scripts": {
    "clean": "rimraf ./dist",
    "build": "npm run clean && swc src -d dist",
    "dev": "cross-env NODE_ENV=development concurrently npm:tsc:watch npm:start:watch",
    "lint": "tsc --noEmit && eslint --fix .",
    "format": "prettier -w .",
    "lint:type": "tsc --noemit",
    "start": "cross-env NODE_ENV=production npm-run-all build start:pm2",
    "start:node": "cross-env NODE_ENV=production node dist/index.js",
    "start:pm2": "pm2 start .pm2.config.js --no-daemon --env production",
    "start:watch": "node -e \"setTimeout(()=>{},5000)\" && nodemon ./dist/src/index.js",
    "tsc:watch": "npm run clean && tsc -w"
  },
  "dependencies": {
    "@discordjs/builders": "1.1.0",
    "@discordjs/rest": "1.0.1",
    "@discordjs/voice": "0.11.0",
    "@fastify/rate-limit": "7.3.0",
    "@vitalets/google-translate-api": "8.0.0",
    "axios": "0.27.2",
    "canvas": "2.9.3",
    "chalk": "4.1.2",
    "cross-env": "7.0.3",
    "crypto-js": "4.1.1",
    "dayjs": "1.11.5",
    "discord-api-types": "0.37.2",
    "discord.js": "14.2.0",
    "dotenv": "16.0.1",
    "fastify": "4.4.0",
    "gifencoder": "2.0.1",
    "glob": "8.0.3",
    "mongoose": "6.5.2",
    "ms": "2.1.3",
    "node-cache": "5.1.2",
    "pidusage": "3.0.0",
    "string-similarity": "4.0.4",
    "urban-dictionary": "3.0.2",
    "xml2js": "0.4.23"
  },
  "devDependencies": {
    "@swc/cli": "0.1.57",
    "@swc/core": "1.2.233",
    "@types/crypto-js": "4.1.1",
    "@types/gifencoder": "2.0.1",
    "@types/glob": "7.2.0",
    "@types/ms": "0.7.31",
    "@types/node": "18.7.3",
    "@types/pidusage": "2.0.2",
    "@types/string-similarity": "4.0.0",
    "@types/urban-dictionary": "3.0.0",
    "@types/xml2js": "0.4.11",
    "@typescript-eslint/eslint-plugin": "5.33.0",
    "@typescript-eslint/parser": "5.33.0",
    "concurrently": "7.3.0",
    "eslint": "8.22.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-promise": "6.0.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "eslint-plugin-sonarjs": "0.15.0",
    "eslint-plugin-unicorn": "43.0.2",
    "nodemon": "2.0.19",
    "npm-run-all": "4.1.5",
    "pm2": "5.2.0",
    "prettier": "2.7.1",
    "rimraf": "3.0.2",
    "typescript": "4.7.4"
  }
}
