{
  "private": true,
  "name": "draconianbot",
  "version": "2.0.07-Alpha",
  "description": "Multi-Purpose & Productivity concentrated Discord BOT.",
  "license": "MIT",
  "author": "RealKoolisw",
  "homepage": "https://github.com/RealKoolisw/DraconianBot#readme",
  "repository": {
    "url": "git+https://github.com/RealKoolisw/DraconianBot.git"
  },
  "bugs": {
    "url": "https://github.com/RealKoolisw/DraconianBot/issues"
  },
  "main": "src/index.ts",
  "scripts": {
    "prebuild": "rimraf ./dist && npm run lint:type",
    "build": "babel --extensions .ts ./src -d ./dist",
    "dev": "concurrently npm:tsc:watch npm:start:watch",
    "lint": "npm-run-all lint:style lint:fix lint:type",
    "lint:fix": "eslint --ext .ts src --fix",
    "lint:style": "prettier --write .",
    "lint:type": "tsc --noemit",
    "start": "cross-env NODE_ENV=production pm2 start .pm2.config.js --no-daemon --env production",
    "start:node": "cross-env NODE_ENV=production node dist/index.js",
    "start:watch": "nodemon dist/src/index.js",
    "tsc:watch": "tsc -w",
    "update": "ncu"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "better-sqlite3": "^7.5.1",
    "chalk": "^4.1.2",
    "core-js": "^3.22.4",
    "cross-env": "^7.0.3",
    "discord.js": "^13.6.0",
    "distube": "^3.3.4",
    "dotenv": "^16.0.0",
    "enmap": "^5.9.0",
    "glob": "^8.0.1",
    "moment": "^2.29.3",
    "ms": "^2.1.3",
    "node-schedule": "^2.1.0",
    "pidusage": "^3.0.0",
    "twemoji-parser": "^14.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.10",
    "@babel/core": "^7.17.10",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-proposal-object-rest-spread": "^7.17.3",
    "@babel/plugin-transform-runtime": "^7.17.10",
    "@babel/preset-env": "^7.17.10",
    "@babel/preset-typescript": "^7.16.7",
    "@types/express": "^4.17.13",
    "@types/glob": "^7.2.0",
    "@types/node-schedule": "^2.1.0",
    "@types/pidusage": "^2.0.2",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-minify": "^0.5.1",
    "concurrently": "^7.1.0",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "nodemon": "^2.0.16",
    "npm-check-updates": "^12.5.11",
    "npm-run-all": "^4.1.5",
    "pm2": "^5.2.0",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.4"
  },
  "engines": {
    "node": ">=16.6.0",
    "npm": ">=8.x"
  }
}
