{
  "private": true,
  "name": "draconian",
  "version": "2.1.0",
  "description": "Multi-Purpose & Productivity concentrated Discord BOT.",
  "license": "MIT",
  "author": "ckt1031",
  "type": "module",
  "main": "src/index.ts",
  "scripts": {
    "dev": "nodemon",
    "format": "prettier -w .",
    "lint": "eslint --fix .",
    "merge-config": "node --loader @esbuild-kit/esm-loader tools/merge-config.ts",
    "start": "cross-env NODE_ENV=production node --loader @esbuild-kit/esm-loader src/index.ts",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@discordjs/builders": "1.5.0",
    "@discordjs/rest": "1.6.0",
    "@esbuild-kit/esm-loader": "2.5.5",
    "@vitalets/google-translate-api": "9.1.0",
    "axios": "1.3.4",
    "canvas": "2.10.2",
    "chalk": "5.2.0",
    "cross-env": "7.0.3",
    "crypto-js": "4.1.1",
    "dayjs": "1.11.7",
    "deepmerge": "4.3.0",
    "discord-api-types": "0.37.36",
    "discord.js": "14.8.0",
    "dotenv": "16.0.3",
    "fastify": "4.14.1",
    "gifencoder": "2.0.1",
    "glob": "9.3.0",
    "mongoose": "7.0.2",
    "ms": "2.1.3",
    "node-cache": "5.1.2",
    "pidusage": "3.0.2",
    "string-similarity": "4.0.4",
    "urban-dictionary": "3.0.2",
    "xml2js": "0.4.23",
    "zod": "3.21.4"
  },
  "devDependencies": {
    "@ckt1031/eslint-config": "0.2.0",
    "@ckt1031/prettier-config": "0.2.0",
    "@ckt1031/tsconfig": "0.2.0",
    "@types/crypto-js": "4.1.1",
    "@types/gifencoder": "2.0.1",
    "@types/ms": "0.7.31",
    "@types/node": "18.15.3",
    "@types/pidusage": "2.0.2",
    "@types/string-similarity": "4.0.0",
    "@types/urban-dictionary": "3.0.0",
    "@types/xml2js": "0.4.11",
    "eslint": "8.36.0",
    "nodemon": "2.0.21",
    "prettier": "2.8.4",
    "typescript": "4.9.5"
  },
  "engines": {
    "node": ">=16.9.0",
    "npm": ">=7"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts",
    "delay": 2500,
    "ignore": [
      "dist",
      "temp",
      "node_modules"
    ],
    "exec": "node --loader @esbuild-kit/esm-loader --inspect src/index.ts"
  },
  "packageManager": "pnpm@7.29.3"
}
